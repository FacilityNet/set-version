{
  "name": "set-version-action",
  "version": "1.0.0",
  "private": true,
  "description": "GitHub Actions - set version info from git tag",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc -p src",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FacilityNet/set-version.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "FacilityNet",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/exec": "^1.0.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/node": "^12.19.4",
    "@typescript-eslint/parser": "^4.0.0",
    "@zeit/ncc": "^0.22.0",
    "eslint": "^7.13.0",
    "eslint-plugin-github": "^4.0.0",
    "eslint-plugin-jest": "^24.0.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^3.14.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  }
}
